---
import { Icon } from 'astro-icon/components'
import { Image } from 'astro:assets'
import MetaTradeImage from '../../../images/mt5_bg_img.png'
import { useTranslations, type Lang } from '@/i18n'
const t = useTranslations(Astro.currentLocale as Lang)
---

<section class="pb-9 pt-12 md:py-14">
	<div class="container">
		<div class="relative">
			<div class="mb-7">
				<h1
					class="text-5xl font-bold text-dbgreen-500 sm:text-6xl"
					data-aos="fade-up-sm"
				>
					<span class="text-neutral-800 dark:text-white">MT5</span>
					{
						t({
							en: 'WebTrader',
							ar: 'ويب تريدر',
							es: 'WebTrader',
							fr: 'WebTrader',
							hi: 'वेबट्रेडर',
							id: 'WebTrader',
							ms: 'WebTrader',
							th: 'เว็บเทรดเดอร์',
							vi: 'WebTrader',
							bn: 'ওয়েবট্রেডার',
							'zh-hans': '网络交易平台',
							'pt-br': 'WebTrader'
						})
					}
				</h1>
			</div>
			<div class="relative z-10 grid grid-cols-6 gap-3">
				<div
					data-aos="fade-up-sm"
					data-aos-delay="100"
					class="relative col-span-full overflow-hidden rounded-2xl border-2 p-4 transition hover:border-dbgreen-900 dark:border-neutral-600 dark:hover:border-dbgreen-900 sm:p-8 md:p-5 lg:col-span-3"
				>
					<div class="h-full">
						<div
							class="relative z-10 flex h-full flex-col justify-between space-y-8 lg:space-y-6"
						>
							<div
								class="relative flex aspect-square size-12 items-center justify-center rounded-full border before:absolute before:-inset-2 before:rounded-full before:border dark:border-white/10 dark:bg-white/5 dark:before:border-white/5 dark:before:bg-white/5"
							>
								<!-- <Icon name="market_icon" class="" href="#" /> -->
								<iconify-icon
									icon="hugeicons:market-analysis"
									width="30"
									height="30"
									class="text-dbgreen-500"
								></iconify-icon>
							</div>
							<div class="space-y-2">
								<h2 class="text-xl font-bold text-neutral-800 dark:text-white">
									{
										t({
											en: 'Daily Market Analysis & Researches',
											ar: 'تحليل وأبحاث السوق اليومية',
											es: 'Análisis y estudios del mercado diario',
											fr: 'Analyse et recherches quotidiennes du marché',
											hi: 'दैनिक बाजार विश्लेषण और अनुसंधान',
											id: 'Analisis Pasar Harian & Riset',
											ms: 'Analisis Pasaran Harian & Penyelidikan',
											th: 'การวิเคราะห์และวิจัยตลาดรายวัน',
											vi: 'Phân tích & Nghiên cứu Thị trường Hàng ngày',
											bn: 'দৈনিক বাজার বিশ্লেষণ এবং গবেষণা',
											'zh-hans': '每日市场分析与研究',
											'pt-br': 'Análise e Pesquisas de Mercado Diárias'
										})
									}
								</h2>
								<p class="text-base text-neutral-700 dark:text-neutral-300">
									{
										t({
											en: 'Insightful analysis on systemic themes underlying the markets and traders perspective on these developments',
											ar: 'تحليل عميق حول المواضيع النظامية الأساسية التي تؤثر على الأسواق ووجهة نظر المتداولين حول هذه التطورات',
											es: 'Análisis perspicaz sobre los temas sistémicos que subyacen en los mercados y la perspectiva de los traders sobre estos desarrollos',
											fr: 'Analyse perspicace des thèmes systémiques sous-jacents aux marchés et perspective des traders sur ces développements',
											hi: 'बाजारों के पीछे के प्रणालीगत विषयों पर जानकारीपूर्ण विश्लेषण और व्यापारियों के दृष्टिकोण पर ये विकास',
											id: 'Analisis mendalam tentang tema sistemik yang mendasari pasar dan perspektif trader tentang perkembangan ini',
											ms: 'Analisis yang mendalam mengenai tema sistemik yang mendasari pasaran dan perspektif pedagang mengenai perkembangan ini',
											th: 'การวิเคราะห์เชิงลึกเกี่ยวกับธีมระบบที่อยู่เบื้องหลังตลาดและมุมมองของผู้ค้าเกี่ยวกับการพัฒนาเหล่านี้',
											vi: 'Phân tích sâu sắc về các chủ đề hệ thống dưới nền tảng của thị trường và quan điểm của các nhà giao dịch về những phát triển này',
											bn: 'বাজারের পিছনের সিস্টেমেটিক থিমগুলির উপর অন্তর্দৃষ্টিপূর্ণ বিশ্লেষণ এবং ট্রেডারদের এই উন্নয়নের উপর দৃষ্টিভঙ্গি',
											'zh-hans':
												'对市场背后系统主题的深刻分析，以及交易者对这些发展的看法',
											'pt-br':
												'Análise perspicaz sobre os temas sistêmicos subjacentes aos mercados e a perspectiva dos traders sobre esses desenvolvimentos'
										})
									}
								</p>
							</div>
						</div>
					</div>
				</div>
				<div
					data-aos="fade-up-sm"
					data-aos-delay="200"
					class="relative col-span-full overflow-hidden rounded-2xl border-2 p-4 hover:border-dbgreen-900 dark:border-neutral-600 dark:hover:border-dbgreen-900 sm:p-8 md:p-5 lg:col-span-3"
				>
					<div class="h-full">
						<div
							class="relative z-10 flex h-full flex-col justify-between space-y-8 lg:space-y-6"
						>
							<div
								class="relative flex aspect-square size-12 items-center justify-center rounded-full border before:absolute before:-inset-2 before:rounded-full before:border dark:border-white/10 dark:bg-white/5 dark:before:border-white/5 dark:before:bg-white/5"
							>
								<!-- <Icon name="market_icon" class="" href="#" /> -->
								<iconify-icon
									icon="cil:education"
									width="30"
									height="30"
									class="text-dbgreen-500"
								></iconify-icon>
							</div>
							<div class="space-y-2">
								<h2 class="text-xl font-bold text-neutral-800 dark:text-white">
									{
										t({
											en: 'Education, Ebooks & Webinars',
											ar: 'التعليم، الكتب الإلكترونية والندوات عبر الإنترنت',
											es: 'Educación, Ebooks y Webinars',
											fr: 'Éducation, Ebooks et Webinaires',
											hi: 'शिक्षा, ईबुक और वेबिनार',
											id: 'Pendidikan, Ebooks & Webinar',
											ms: 'Pendidikan, Ebooks & Webinar',
											th: 'การศึกษา, Ebooks และการสัมมนาผ่านเว็บ',
											vi: 'Giáo dục, Sách điện tử & Hội thảo trực tuyến',
											bn: 'শিক্ষা, ইবুক এবং ওয়েবিনার',
											'zh-hans': '教育、电子书和网络研讨会',
											'pt-br': 'Educação, Ebooks e Webinars'
										})
									}
								</h2>
								<p class="text-base text-neutral-700 dark:text-neutral-300">
									{
										t({
											en: 'Whatever your skill level, we’ve got the tools and resources to help improve your trading.',
											ar: 'بغض النظر عن مستوى مهارتك، لدينا الأدوات والموارد لمساعدتك في تحسين تداولك.',
											es: 'Cualquiera que sea tu nivel de habilidad, tenemos las herramientas y recursos para ayudarte a mejorar tu trading.',
											fr: 'Quel que soit votre niveau de compétence, nous avons les outils et les ressources pour vous aider à améliorer votre trading.',
											hi: 'आपकी कौशल स्तर चाहे जो भी हो, हमारे पास आपके ट्रेडिंग को सुधारने के लिए उपकरण और संसाधन हैं।',
											id: 'Apa pun tingkat keterampilan Anda, kami memiliki alat dan sumber daya untuk membantu meningkatkan perdagangan Anda.',
											ms: 'Apa jua tahap kemahiran anda, kami mempunyai alat dan sumber untuk membantu meningkatkan perdagangan anda.',
											th: 'ไม่ว่าคุณจะมีระดับทักษะใด เรามีเครื่องมือและทรัพยากรที่จะช่วยปรับปรุงการซื้อขายของคุณ',
											vi: 'Dù bạn có kỹ năng ở mức độ nào, chúng tôi cũng có công cụ và tài nguyên để giúp cải thiện giao dịch của bạn.',
											bn: 'আপনার দক্ষতার স্তর যাই হোক না কেন, আপনার ট্রেডিং উন্নত করতে আমাদের কাছে সরঞ্জাম এবং সম্পদ রয়েছে।',
											'zh-hans':
												'无论您的技能水平如何，我们都有工具和资源来帮助改善您的交易。',
											'pt-br':
												'Independentemente do seu nível de habilidade, temos as ferramentas e recursos para ajudá-lo a melhorar sua negociação.'
										})
									}
								</p>
							</div>
						</div>
					</div>
				</div>
				<div
					data-aos="fade-up-sm"
					data-aos-delay="250"
					class="relative col-span-full overflow-hidden rounded-2xl border-2 p-4 transition hover:border-dbgreen-900 dark:border-neutral-600 dark:hover:border-dbgreen-900 sm:p-8 md:p-5 lg:col-span-6"
				>
					<div class="h-full">
						<div
							class="relative z-10 flex h-full flex-col justify-between space-y-8 lg:space-y-6"
						>
							<div class="max-w-full space-y-2 md:max-w-[60%]">
